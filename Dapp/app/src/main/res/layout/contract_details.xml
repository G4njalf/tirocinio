<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/addressTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Address:"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/premioTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Premio:"
        app:layout_constraintTop_toBottomOf="@id/addressTextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/isLiquidatoTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Liquidato:"
        app:layout_constraintTop_toBottomOf="@id/premioTextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/isAttivatoTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Attivato:"
        app:layout_constraintTop_toBottomOf="@id/isLiquidatoTextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/isFundendTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fundend:"
        app:layout_constraintTop_toBottomOf="@id/isAttivatoTextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/addressAssicuratoTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Assicurato:"
        app:layout_constraintTop_toBottomOf="@id/isFundendTextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/addressAssicuratoreTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Assicuratore:"
        app:layout_constraintTop_toBottomOf="@id/addressAssicuratoTextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>